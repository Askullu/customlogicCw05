{
  "name": "BusinessPartnerValidation",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "license": "ISC",
  "dependencies": {
    "@sap/cds": "^3.34.1",
    "@sap/hana-client": "^2.4.182",
    "@sap/xb-msg-amqp-v100": "^0.9.39",
    "express": "^4",
    "grunt": "^1.1.0"
  },
  "scripts": {
    "start": "npx cds run",
    "test": "jest"
  },
  "devDependencies": {
    "sqlite3": "^4.1.1",
    "jest": "*"
  },
  "cds": {
    "requires": {
      "[production]": {
        "db": {
          "kind": "hana",
          "model": [
            "db",
            "srv"
          ]
        },
        "messaging": {
          "kind": "enterprise-messaging"
        }
      },
      "API_BUSINESS_PARTNER": {
        "kind": "odata",
        "model": "srv/external/API_BUSINESS_PARTNER",
        "[production]": {
          "credentials": {
            "destination": "s4h-basic",
            "requestTimeout": 18000000,
            "prefix": "refapps/bpems/abc/S4H/BO"
          }
        }
      }
    }
  }
}
